  "license": "ISC",
  "dependencies": {
    "@google/generative-ai": "^0.24.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.14.1",
    "ai": "^4.3.7",
    "dotenv": "^16.5.0",
    "fs-extra": "^11.3.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "zod": "^3.24.2"
  }
}
