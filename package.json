{
  "name": "my-coding-agent",
  "version": "1.0.0",
  "description": "A coding assistant AI agent built with Google's Gemini Pro",
  "main": "dist/index.js",
  "scripts": {
    "start": "tsx src/index.ts",
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "chat": "node -e \"require('child_process').spawn('tsx', ['src/index.ts', ...process.argv.slice(2)], {stdio: 'inherit'})\""
